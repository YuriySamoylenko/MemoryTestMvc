    <script src="@Url.Content("~/Scripts/jquery-1.5.1.js")" type="text/javascript"></script>
    
    <link href="@Url.Content("~/Scripts/css/ui-lightness/jquery-ui-1.8.24.custom.css")" rel="stylesheet" type="text/css" />
