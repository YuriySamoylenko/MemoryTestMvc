<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    @*<link type="text/css" href="~/Scripts/css/ui-lightness/jquery-ui-1.8.24.custom.css" rel="Stylesheet" />
    <script type="text/javascript" src="~/Scripts/js/jquery-1.8.2.min.js"></script>
